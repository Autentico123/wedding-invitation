{
  "buildCommand": "npm run build",
  "installCommand": "npm install --legacy-peer-deps",
  "outputDirectory": "dist",
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/submit",
      "destination": "/api/submit.js"
    },
    {
      "source": "/api/health",
      "destination": "/api/health.js"
    }
  ]
}
